{
  "name": "mx-document-generator",
  "version": "1.0.5",
  "description": "Library to generate RFC and CURP",
  "main": "dist/lib/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smaniotov/RFC-CURP-Mexico.git"
  },
  "keywords": [
    "rfc",
    "curp",
    "mx",
    "mexico-documents",
    "document"
  ],
  "author": "Héctor Iván Patricio & Vinícius O. Smanioto",
  "license": "ISC",
  "homepage": "https://github.com/smaniotov/RFC-CURP-Mexico#readme",
  "jest": {
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.test.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.13",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "5.16.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.3.3",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5"
  }
}
