{
  "name": "paybook-ts",
  "version": "1.1.1",
  "description": "Paybook API helper",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "deploy": "npm run build && npm publish"
  },
  "keywords": [
    "paybook",
    "sync",
    "typescript"
  ],
  "author": "Gustavo de B. Lima",
  "license": "ISC",
  "jest": {
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.test.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "axios": "0.19.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.13",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "5.16.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.3.3",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5"
  }
}
